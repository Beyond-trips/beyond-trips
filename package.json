{
  "name": "beyond-trips",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "create-admin": "tsx createAdmin.ts",
    "seed": "tsx scripts/seed.ts",
    "seed:plans": "tsx src/seeds/subscriptionPlans.ts",
    "migrate:s3": "tsx scripts/migrate-to-s3.ts",
    "migrate:campaign-media": "tsx scripts/migrate-campaign-media.ts",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:swagger": "tsx scripts/generate-swagger.ts",
    "generate:postman": "tsx scripts/generate-postman-from-docs.ts",
    "generate:postman:code": "tsx scripts/generate-postman-collection.ts",
    "generate:postman:experimental": "tsx scripts/generate-postman-with-validation.ts",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "jest --verbose",
    "test:all": "jest --verbose",
    "test:admin": "jest __tests__/admin --verbose",
    "test:advertiser": "jest __tests__/advertiser --verbose",
    "test:driver": "jest __tests__/driver --verbose",
    "test:rider": "jest __tests__/rider --verbose",
    "test:qr": "jest __tests__/qr-scanning --verbose",
    "test:system": "jest __tests__/system --verbose",
    "test:seed": "tsx __tests__/seed-test-data.ts",
    "test:coverage": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:legacy": "jest __tests__/driver-dashboard.test.ts __tests__/advertiser-dashboard.test.ts __tests__/admin-dashboard.test.ts --verbose",
    "test:advanced": "jest __tests__/admin-advanced.test.ts --verbose",
    "test:premium": "jest __tests__/admin-premium.test.ts --verbose",
    "test:polish": "jest __tests__/polish.test.ts --verbose",
    "test:prd-missing": "jest __tests__/prd-missing-features.test.ts --verbose",
    "test:barcode": "jest __tests__/barcode-scanning.test.ts --verbose",
    "test:advertiser-qr": "jest __tests__/advertiser-qr.test.ts --verbose"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@payloadcms/db-mongodb": "3.33.0",
    "@payloadcms/next": "3.33.0",
    "@payloadcms/payload-cloud": "3.33.0",
    "@payloadcms/plugin-cloud-storage": "3.33.0",
    "@payloadcms/richtext-lexical": "3.33.0",
    "@payloadcms/storage-s3": "3.33.0",
    "@payloadcms/storage-uploadthing": "3.33.0",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "next": "15.3.0",
    "nodemailer": "^7.0.3",
    "payload": "3.33.0",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.32.6",
    "stripe": "^19.3.1",
    "uploadthing": "^7.7.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.0.0",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
